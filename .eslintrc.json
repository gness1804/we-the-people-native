{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "ecmafeatures":{
    "arrowFunctions": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": "off",
    "no-alert": "off",
    "no-extra-semi": "off",
    "no-unused-vars": "off",
    "semi": "off"
  },
  "parser": "babel-eslint"
}
