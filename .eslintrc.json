{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "ecmafeatures":{
    "arrowFunctions": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "jsx-quotes": "off",
    "no-alert": "off",
    "no-extra-semi": "off",
    "no-unused-vars": "off",
    "react/prefer-stateless-function": "off",
    "semi": "off"
  },
  "parser": "babel-eslint"
}
